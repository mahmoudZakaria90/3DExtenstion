{
    "name": "bookmarks",
    "version": "1.0",
    "description": "ChromeAPI-bookmarks",
    "manifest_version": 2,
	"background": {
	  "scripts": ["background.js"],
	  "persistent": false
	},
	"permissions": ["tabs", "bookmarks", "declarativeContent"],
	"page_action": {
		"default_popup": "popup.html"
	}
  }