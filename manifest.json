{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
	"background": {
	  "scripts": ["background.js"],
	  "persistent": true
	},
	"permissions": ["activeTab", "tabs", "declarativeContent", "*://*/*", "storage"],
	"page_action": {
		"default_popup": "popup.html"
	}
  }