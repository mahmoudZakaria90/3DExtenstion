{
  "name": "bookmarky",
  "version": "1.1.4",
  "description": "ChromeAPI-bookmarks",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["bookmarks", "declarativeContent"],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icons/16.png"
    }
  },
  "icons": {
    "48": "assets/icons/48.png"
  }
}
